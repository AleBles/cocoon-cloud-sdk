{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "out": "./dist/cocoon.sdk.js",
        "sourceMap": true,
        "declaration": true
    },
    "filesGlob": [
        "./src/*.ts",
        "!./node_modules/**/*.ts",
        "!./test/**/*.ts",
        "!./dist/**/*.ts"
    ]
}